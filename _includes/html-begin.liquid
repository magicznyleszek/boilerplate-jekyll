<!DOCTYPE html>
<html class="pterodactylus crushed my algorithms" lang="en">

<head>

    <!-- stuff -->
    <meta charset="utf-8">
    <meta http-equiv="cleartype" content="on">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <!-- info -->
    <title>{{ site.name }}</title>
    <meta name="keywords" content="{{ site.keywords }}">
    <meta name="description" content="{{ site.description }}">
    <meta name="author" content="{{ site.author }}">
    <link rel="author" href="humans.txt">
    <link rel="home" href="{{ site.baseurl }}">

    <!-- open graph -->
    <meta property="og:type" content="website">
    <meta property="og:site_name" content="{{ site.name }}">
    <meta property="og:title" content="{{ page.title }}">
    <meta property="og:description" content="{{ site.description }}">
    <meta property="og:url" content="{{ site.baseurl }}">
    <meta property="og:image" content="{{ site.baseurl }}/assets/img/image-200.png"/>

    <!-- temp styles -->
    <link href="http://boilerplate-assets.smutnyleszek.com/assets/css/main.css?version=1" media="all" rel="stylesheet">
    <link href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:400,700,400italic,700italic&amp;subset=latin,latin-ext" rel="stylesheet">
    <link href="http://fonts.googleapis.com/css?family=Source+Code+Pro" rel="stylesheet">
    <!-- / temp styles -->

    {% comment %}
    <!-- style & viewport -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="{{ site.baseurl }}/assets/css/main.css?version=1" media="all" rel="stylesheet">

    <!-- icons -->
    <link rel="icon" href="{{ site.baseurl }}/assets/img/favicon.ico?version=1">
    <link rel="apple-touch-icon" href="{{ site.baseurl }}/assets/img/apple-touch-icon.png?version=1">

    <!-- fonts -->
    <link href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:400,700,400italic,700italic&subset=latin,latin-ext" rel="stylesheet">
    <link href="http://fonts.googleapis.com/css?family=Source+Code+Pro" rel="stylesheet">
    <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
    {% endcomment %}

</head>

<body>
    {% include icons.svg %}
